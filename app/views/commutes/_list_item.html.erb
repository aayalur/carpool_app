<li class="clearfix">
  <div class="warnings">
    <%= image_tag "icons/exclamation.png", :alt => "Warning: No Commutes", :class => "commutes_none" %>
    <%= image_tag "icons/exclamation-red.png", :alt => "Warning: Commute Cancellation", :class => "commutes_cancellation" %>
  </div>
  <div class="depart_from">
    <span class="marker_way_point"><b>A</b></span>
    [location_a]
  </div>
  <div class="arrive_to">
    <span class="marker_way_point"><b>B</b></span>
    [location_a]
  </div>
  <div class="arrive_for">
    <span class="marker_time"></span>
    [time]
  </div>
  <div class="passengers">
    <span class="marker_passengers"></span>
    [passengers]
  </div>
  <%= link_to content_tag(:b, "Go"), "", :title => "Go", :class => "go" %>
</li>